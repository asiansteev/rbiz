<%= extension_point('/common/header') %>

<%= will_paginate @customers %>
<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Last Login</th>
      <th># Orders</th>
    </tr>
  </thead>
  <tbody>
    <% @customers.each do |customer| -%>
      <tr>
        <td><%= customer.email %></td>
        <td><%= customer.last_login %></td>
        <td><%= customer.carts.count %></td>
      </tr>
    <% end -%>
  </tbody>
</table>
<%= will_paginate @customers %>

<%= extension_point('/common/footer') %>
